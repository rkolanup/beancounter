    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.0.1",
