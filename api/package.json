{
  "name": "api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "build": "npx tsc",
    "typeorm": "npm run build && npx typeorm",
    "migration:create": "typeorm migration:create",
    "migration:generate": "npm run typeorm -- -d dist/db/data-source.js migration:generate",
    "migration:run": "npm run typeorm -- -d dist/db/data-source.js migration:run",
    "migration:revert": "npm run typeorm -- -d dist/db/data-source.js migration:revert",
    "start": "node dist/server.js",
    "start:dev": "nodemon --exec ts-node src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "moment": "^2.30.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
